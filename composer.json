{
  "name": "yangdong/migration",
  "version": "1.0.0",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "migration"
  ],
  "description": "Migration database for swoft framework",
  "license": "Apache-2.0",
  "require": {
    "ext-pdo": "*",
    "swoft/framework": "^1.0.15",
    "swoft/console": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Phinx\\": "phinx/src/Phinx",
      "Swoft\\Migration\\": "src/",
      "Cake\\Collection\\": "collection/"
    },
    "files": [
      "collection/functions.php"
    ]
  },
  "require-dev": {
    "swoft/swoole-ide-helper": "dev-master"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  ]
}
